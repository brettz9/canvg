{
  "name": "canvg",
  "description": "Javascript SVG parser and renderer on Canvas",
  "homepage": "https://github.com/canvg/canvg",
  "bugs": "https://github.com/canvg/canvg/issues",
  "engines": {
    "node": "*"
  },
  "keywords": [
    "javascript",
    "client",
    "browser",
    "svg",
    "canvas"
  ],
  "author": "Gabe Lerner <gabelerner@gmail.com>",
  "contributors": [
    "Brett Zamir"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/canvg/canvg.git"
  },
  "main": "dist/node/canvg.js",
  "types": "dist/node/canvg.d.ts",
  "browser": "dist/browser/canvg.min.js",
  "module": "dist/browser/canvg-es.js",
  "version": "2.0.0",
  "scripts": {
    "test-browser": "ava test/browser.test.js --tap | tap-diff",
    "test-node": "ava test/node.test.js --tap | tap-diff",
    "test-common": "ava test/common.test.js --tap | tap-diff",
    "test": "npm run test-common && npm run test-node",
    "minify-browser": "IS_MIN=1 rollup -c",
    "minify": "npm run minify-browser",
    "eslint": "eslint --report-unused-disable-directives .",
    "build-browser": "rollup -c",
    "build-es": "IS_ES=1 rollup -c",
    "build-node": "IS_NODE=1 rollup -c",
    "build-win": "npm run build-node-win && npm run minify",
    "build-node-win": "cmd /C \"set IS_NODE=1 && rollup -c\"",
    "build": "npm run eslint && npm run build-browser && npm run build-es && npm run build-node && npm run minify",
    "generate-expected": "node test/_generate_expected_node.js",
    "start": "node test/_server.js"
  },
  "license": "MIT",
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "readmeFilename": "README.md",
  "browserslist": "cover 100%",
  "dependencies": {
    "jsdom": "^13.0.0",
    "rgbcolor": "^1.0.1",
    "stackblur-canvas": "^2.0.0",
    "xmldom": "^0.1.27"
  },
  "peerDependencies": {
    "canvas": "^2.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "ava": "^1.0.0-rc.2",
    "ava-is-running": "^1.0.3",
    "blink-diff": "^1.0.13",
    "bluebird": "^3.5.3",
    "canvas": "^2.1.0",
    "data-uri-to-buffer": "^2.0.0",
    "debug": "^4.1.0",
    "eslint": "^5.9.0",
    "eslint-config-ash-nazg": "0.1.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-compat": "^2.6.3",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsdoc": "git+https://github.com/brettz9/eslint-plugin-jsdoc.git#Deploy",
    "eslint-plugin-markdown": "^1.0.0-rc.0",
    "eslint-plugin-no-use-extend-native": "^0.3.12",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-unicorn": "^6.0.1",
    "esm": "^3.0.84",
    "mkdirp": "^0.5.1",
    "pngjs-image": "^0.11.7",
    "puppeteer": "^1.10.0",
    "rollup": "^0.67.3",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-terser": "^3.0.0",
    "serve-handler": "^5.0.7",
    "tap-diff": "^0.1.1",
    "uglify-js": "^3.4.9"
  },
  "jspm": {
    "registry": "npm"
  }
}
